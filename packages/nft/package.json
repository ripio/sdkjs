{
    "name": "@ripio/sdk-nft",
    "version": "1.0.0",
    "description": "Tools to interact with a NFT",
    "files": [
      "dist"
    ],
    "main": "dist/cjs/index.js",
    "types": "dist/types/index.d.ts",
    "exports":{
      ".": {
        "types": "./dist/types/index.d.ts",
        "require": "./dist/cjs/index.js",
        "import": "./dist/esm/index.js",
        "default": "./dist/esm/index.js"
      },
      "./types": {
        "types": "./dist/types/types/index.d.ts",
        "require": "./dist/cjs/types/index.js",
        "import": "./dist/esm/types/index.js",
        "default": "./dist/esm/types/index.js"
      },
      "./storage": {
        "types": "./dist/types/nft/storage/index.d.ts",
        "require": "./dist/cjs/nft/storage/index.js",
        "import": "./dist/esm/nft/storage/index.js",
        "default": "./dist/esm/nft/storage/index.js"
      }
    },
    "typesVersions": {
      "*": {
        ".": [
          "dist/types/index.d.ts"
        ],
        "types": [
          "dist/types/types/index.d.ts"
        ],
        "storage": [
          "dist/types/storage/index.d.ts"
        ]
      }
    },
    "scripts": {
      "type-check": "tsc --noEmit",
      "type-check:watch": "npm run type-check -- --watch",
      "build": "npm run build:esm && npm run build:cjs",
      "build:esm": "tsc --project tsconfig.esm.json",
      "build:cjs": "tsc --project tsconfig.cjs.json",
      "lint:check": "eslint --ext .ts src",
      "lint:fix": "eslint --ext .ts --fix src",
      "format:check": "prettier --check \"./**/*.ts\" src",
      "format:fix": "prettier --write \"./**/*.ts\" src",
      "test": "jest",
      "test-watch": "jest --watch",
      "test-init": "jest --init",
      "test-cov": "jest --ci --coverage --reporters=default --reporters=jest-junit",
      "ts:defs": "tsc --declaration --outDir . --emitDeclarationOnly"
    },
    "repository": {
      "type": "git",
      "url": "git+https://github.com/ripio/sdkjs.git"
    },
    "license": "Apache 2.0",
    "publishConfig": {
      "access": "public",
      "types": "dist/types"
    },
    "dependencies": {
        "@babel/runtime": "^7.17.7",
        "@ripio/sdk":"*"
    }
}