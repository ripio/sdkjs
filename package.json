{
  "name": "@ripio/sdk",
  "version": "1.0.0",
  "description": "SDK to interact with Smart Contracts and wallets",
  "files": [
    "dist"
  ],
  "main": "dist/cjs/index.js",
  "types": "dist/types/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/types/index.d.ts",
      "require": "./dist/cjs/index.js",
      "import": "./dist/esm/index.js",
      "default": "./dist/esm/index.js"
    },
    "./managers": {
      "types": "./dist/types/managers/index.d.ts",
      "require": "./dist/cjs/managers/index.js",
      "import": "./dist/esm/managers/index.js",
      "default": "./dist/esm/managers/index.js"
    },
    "./connectors": {
      "types": "./dist/types/connectors/index.d.ts",
      "require": "./dist/cjs/connectors/index.js",
      "import": "./dist/esm/connectors/index.js",
      "default": "./dist/esm/connectors/index.js"
    },
    "./utils": {
      "types": "./dist/types/utils/index.d.ts",
      "require": "./dist/cjs/utils/index.js",
      "import": "./dist/esm/utils/index.js",
      "default": "./dist/esm/utils/index.js"
    },
    "./types": {
      "types": "./dist/types/types/index.d.ts",
      "require": "./dist/cjs/types/index.js",
      "import": "./dist/esm/types/index.js",
      "default": "./dist/esm/types/index.js"
    },
    "./gaming": {
      "types": "./dist/types/gaming/index.d.ts",
      "require": "./dist/cjs/gaming/index.js",
      "import": "./dist/esm/gaming/index.js",
      "default": "./dist/esm/gaming/index.js"
    },
    "./ethers-utils": {
      "types": "./dist/types/utils/ethersUtils.d.ts",
      "require": "./dist/cjs/utils/ethersUtils.js",
      "import": "./dist/esm/utils/ethersUtils.js",
      "default": "./dist/esm/utils/ethersUtils.js"
    },
    "./nft/storage": {
      "types": "./dist/types/nft/storage/index.d.ts",
      "require": "./dist/cjs/nft/storage/index.js",
      "import": "./dist/esm/nft/storage/index.js",
      "default": "./dist/esm/nft/storage/index.js"
    }
  },
  "typesVersions": {
    "*": {
      ".": [
        "dist/types/index.d.ts"
      ],
      "managers": [
        "dist/types/managers/index.d.ts"
      ],
      "connectors": [
        "dist/types/connectors/index.d.ts"
      ],
      "utils": [
        "dist/types/utils/index.d.ts"
      ],
      "types": [
        "dist/types/types/index.d.ts"
      ],
      "gaming": [
        "dist/types/gaming/index.d.ts"
      ],
      "ethers-utils": [
        "dist/types/utils/ethersUtils.d.ts"
      ]
    }
  },
  "scripts": {
    "type-check": "tsc --noEmit",
    "type-check:watch": "npm run type-check -- --watch",
    "build": "npm run build:esm && npm run build:cjs",
    "build:esm": "tsc --project tsconfig.esm.json",
    "build:cjs": "tsc --project tsconfig.cjs.json",
    "lint:check": "eslint --ext .ts --ignore-path .gitignore src",
    "lint:fix": "eslint --ext .ts --fix --ignore-path .gitignore src",
    "format:check": "prettier --check --ignore-path .gitignore --ignore-path jest.config.js \"./**/*.ts\" src",
    "format:fix": "prettier --write --ignore-path .gitignore --ignore-path jest.config.js \"./**/*.ts\" src",
    "test": "jest",
    "test-watch": "jest --watch",
    "test-init": "jest --init",
    "test-cov": "jest --ci --coverage --reporters=default --reporters=jest-junit",
    "ts:defs": "tsc --declaration --outDir . --emitDeclarationOnly"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ripio/sdkjs.git"
  },
  "license": "Apache 2.0",
  "publishConfig": {
    "access": "public",
    "types": "dist/types"
  },
  "release": {
    "branches": [
      "+([0-9])?(.{+([0-9]),x}).x",
      "main",
      {
        "name": "develop",
        "prerelease": "dev"
      }
    ],
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/release-notes-generator",
      "@semantic-release/npm",
      "@semantic-release/changelog",
      [
        "@semantic-release/git",
        {
          "assets": [
            "CHANGELOG.md"
          ],
          "message": "chore(release): ${nextRelease.version} [skip ci]\n\n${nextRelease.notes}"
        }
      ],
      [
        "@semantic-release/changelog",
        {
          "changelogFile": "CHANGELOG.md",
          "changelogTitle": "# Versioning Changelog"
        }
      ]
    ]
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.197.0",
    "@babel/runtime": "^7.17.7",
    "ethers": "^5.6.4",
    "ipfs-http-client": "^56.0.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.17.6",
    "@babel/core": "^7.17.7",
    "@babel/plugin-proposal-class-properties": "^7.16.7",
    "@babel/plugin-transform-runtime": "^7.17.0",
    "@babel/preset-env": "^7.16.11",
    "@babel/preset-typescript": "^7.16.7",
    "@semantic-release/changelog": "^6.0.1",
    "@semantic-release/git": "^10.0.1",
    "@semantic-release/npm": "^9.0.1",
    "@types/jest": "^27.4.1",
    "@typescript-eslint/eslint-plugin": "^5.15.0",
    "@typescript-eslint/parser": "^5.15.0",
    "eslint": "^8.9.0",
    "eslint-config-prettier": "^8.4.0",
    "eslint-plugin-prettier": "^4.0.0",
    "ganache": "^7.0.3",
    "jest": "^27.5.1",
    "jest-junit": "^13.2.0",
    "prettier": "^2.5.1",
    "semantic-release": "^19.0.5",
    "ts-jest": "^27.1.3",
    "ts-node": "^10.7.0",
    "typescript": "^4.7.4"
  }
}
